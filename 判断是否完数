#include<stdio.h>
int main()
{
    int number,i,sum;
    for(number=2;number<=1000;number++)
    {sum=1;
    for(i=2;i<=number-1;i++)
    {if(number%i==0)sum+=i;}
    if(number==sum)
    {printf("%d its factors are 1,",number);
    for(i=2;i<=number-1;i++)
    if(number%i==0)
    printf("%d,",i);
    putchar('\n');}}
    return 0;
}
